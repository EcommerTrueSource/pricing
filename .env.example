# Ambiente
NODE_ENV=development

# Configurações do Banco de Dados
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_NAME=contract_management

# Google Cloud SQL
INSTANCE_CONNECTION_NAME=your-project:your-region:your-instance
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/service-account.json

# JWT
JWT_SECRET=your-super-secret-key-change-in-production
JWT_EXPIRATION=1d

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_CALLBACK_URL=http://localhost:3000/api/auth/google/callback
FRONTEND_URL=http://localhost:4200

# APIs Externas
WHATSAPP_API_KEY=your-whatsapp-api-key
WHATSAPP_API_URL=https://api.whatsapp.com/v1

EMAIL_API_KEY=your-email-api-key
EMAIL_API_URL=https://api.email-service.com/v1

SMS_API_KEY=your-sms-api-key
SMS_API_URL=https://api.sms-service.com/v1

SIGNATURE_API_KEY=your-signature-api-key
SIGNATURE_API_URL=https://api.signature-service.com/v1

# Application
PORT=3000
API_PREFIX=api

# Configuração do Autentique
AUTENTIQUE_API_URL=https://api.autentique.com.br/v1
AUTENTIQUE_API_KEY=your_api_key_here

# Configuração da API CNPJWS
CNPJWS_API_URL=https://comercial.cnpj.ws/cnpj
CNPJWS_API_TOKEN=your_cnpjws_token_here

# Configuração da Brasil API (fallback)
BRASIL_API_BASE_URL=https://brasilapi.com.br/api

# Configuração do Banco de Dados
DATABASE_URL=postgresql://user:password@localhost:5432/contract-management
